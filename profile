
# put the current directory in the path so you don't have to type
# ./ in front of executables in the current directory
PATH=$PATH:.

# MacPorts Installer addition on 2010-09-01_at_16:02:47: adding an appropriate PATH variable for use with MacPorts.
PATH=/opt/local/bin:/opt/local/sbin:$PATH
# Finished adapting your PATH environment variable for use with MacPorts.

export PATH
